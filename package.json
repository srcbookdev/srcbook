{
  "name": "srcbook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-api": "pnpm --filter api dev",
    "dev-web": "pnpm --filter web dev",
    "lint-web": "pnpm --filter web lint",
    "check-types": "pnpm --recursive check-types",
    "check-prettier": "pnpm prettier --check .",
    "generate": "cd packages/api && npm run generate --",
    "migrate": "cd packages/api && npm run migrate --"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
